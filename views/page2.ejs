        <body>
            <%- include('./partials/header') %>
          <section class="agile-volt">
            <div class="agile-voltheader">
              <h1>Photography Application form</h1>
            </div>
            <div class="agile-voltsub">
              <h2>Application form</h2>
                <form action="#" method="post">
                  <div class="agile-name">
                    <p>Name :</p>
                    <input type="text" name="username" placeholder="" required="" autofocus>
                  </div>
                  <div class="agile-name">
                    <p>Business name :</p>
                    <input type="text" name="username" placeholder="" required="" autofocus>
                  </div>
                  
                  <div class="agile-email">
                    <p>E-mail :</p>
                    <input type="email" name="email" placeholder="" required="">
                  </div>
                
                  <div class="agile-address">
                    <p>Address :</p>
                    <input type="text" name="address" placeholder="" required="" >
                  </div>
                  <div class="clear"></div>
                  <div class="agile-city">
                    <p>City :</p>
                    <input type="text" name="city" placeholder="" min="1" max="12"/>
                  </div>
                  
                  <div class="agile-country">
                    <p>State</p>
                    <select>
                      <option value="country"></option>
                      <option value="country">State-1</option>
                      <option value="country">State-2</option>
                    </select>
                  </div>
                  
                  <div class="agile-pin">
                    <p>Zipcode :</p>
                    <input type="tel" name="zipcode" placeholder="" />
                  </div>
                  <div class="clear"></div>
                  <div class="agile-name">
                    <p>Phone number :</p>
                    <input type="text" name="businessname" placeholder="" required="" autofocus>
                  </div>
                  
                  <div class="agile-work">
                    <p>What type of categories do you work ?</p>
                      <div class="agile-days">
                        <span class="Monday">
                          <input type="checkbox" name="week" value="monday">Nature Photography<br>  
                        </span>
                        <span class="Tuesday">
                          <input type="checkbox" name="week" value="Tuesday">Abstract Photography<br>         
                        </span>
                        <span class="Wednesday">
                          <input type="checkbox" name="week" value="Wednesday">Portrait Photography<br>
                        </span>
                        <span class="Thursday">
                          <input type="checkbox" name="week" value="thursday">Fashion Photography<br>
                        </span>
                        <span class="Friday">
                          <input type="checkbox" name="week" value="friday">Wildlife photography<br>
                        </span>
                        <span class="Saturday">
                          <input type="checkbox" name="week" value="saturday">Advertising Photography<br>
                        </span>
                        <span class="Sunday">
                          <input type="checkbox" name="week" value="sunday">Wedding Photography<br>
                        </span>
                      </div>
                  </div>
                  
                  <div class="agile-week">
                    <p>Description :</p>
                    <textarea></textarea>
                  </div>
                  <div class="clear"></div>
                  <input type="submit" value="submit" />
                </form>
            </div>
    
          </section>
</body>
</html>


